<h1>Listing users</h1> 
<table class="table table-hover">
  <thead>
    <tr>

      <th>ID</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
   </thead>

   <tbody>
   	<% @users.each do |user|%>
   	<tr>
   	<td><%= user.id %></td>
   	<td><%= user.email %></td>
   	</tr>	
   	<% end %>



   </tbody>

 
</table>
<%= link_to 'New User', new_admin_user_path %>